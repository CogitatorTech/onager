# group: [onager]


require onager
# Test suite for Onager centrality algorithms

statement ok
pragma enable_verification

# Create test edge data
statement ok
create table test_edges as select * from (values
  (1::bigint, 2::bigint), (2, 3), (3, 1), (3, 4)
) t(src, dst)

# Test PageRank returns results
query I
select count(*) > 0 from onager_ctr_pagerank((select src, dst from test_edges))
----
1

# Test Degree Centrality
query I
select count(*) > 0 from onager_ctr_degree((select src, dst from test_edges))
----
1

# Test Betweenness Centrality
query I
select count(*) > 0 from onager_ctr_betweenness((select src, dst from test_edges))
----
1

# Test Closeness Centrality
query I
select count(*) > 0 from onager_ctr_closeness((select src, dst from test_edges))
----
1

# Test Harmonic Centrality
query I
select count(*) > 0 from onager_ctr_harmonic((select src, dst from test_edges))
----
1

# Test Katz Centrality
query I
select count(*) > 0 from onager_ctr_katz((select src, dst from test_edges), alpha := 0.1)
----
1

# Test Eigenvector Centrality
query I
select count(*) > 0 from onager_ctr_eigenvector((select src, dst from test_edges))
----
1

# Test VoteRank
query I
select count(*) > 0 from onager_ctr_voterank((select src, dst from test_edges), num_seeds := 3)
----
1

# Cleanup
statement ok
drop table test_edges
