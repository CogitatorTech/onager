# group: [gaggle]

# Type shape and return type tests for Gaggle

statement ok
pragma enable_verification

# Load the Gaggle extension
statement ok
load 'build/release/extension/gaggle/gaggle.duckdb_extension'

# Return types for scalar functions
query T
select typeof(gaggle_version())
----
VARCHAR

query T
select typeof(gaggle_clear_cache())
----
BOOLEAN

query T
select typeof(gaggle_cache_info())
----
VARCHAR

query T
select typeof(gaggle_file_path('owner/dataset','file.csv'))
----
VARCHAR

